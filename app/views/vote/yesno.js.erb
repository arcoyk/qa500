var yesno = "<%= @params[:yesno] %>";
if (yesno == "yes") {
	$('#yes-<%= @params[:id] %>').text('<%= params[:yescount] %>');
} else if (yesno == "no") {
	$('#no-<%= @params[:id] %>').text('<%= params[:nocount] %>');	
}
$('#answer-input-anstype').val(yesno);
$('.display-<%= @params[:question_id] %>').css('display', 'block');
